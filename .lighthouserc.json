{
  "ci": {
    "collect": {
      "staticDistDir": "./.next/"
    },
    "assert": {
      "preset": [
        "lighthouse:recommended",
        "lighthouse:no-pwa"
      ],
      "assertions": {
        "bf-cache": "off",
        "csp-xss": "off",
        "tap-targets": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}